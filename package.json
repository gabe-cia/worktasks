{
	"name": "worktasks",
	"version": "1.0.0",
	"description": "Manage tasks within a work day",
	"main": "app.js",
	"scripts": {
		"start": "nodemon ./index.js",
		"test": "jest --runInBand",
        "db-build": "npx sequelize db:migrate",
        "db-populate": "npx sequelize db:seed:all",
        "db-wipe": "npx sequelize db:seed:undo:all",
        "db-destroy": "npx sequelize db:migrate:undo:all"
	},
	"keywords": [
		"task",
		"task-manager",
		"test-project"
	],
	"author": "gabriel.nascimento",
	"license": "ISC",
	"dependencies": {
		"amqplib": "^0.8.0",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"express-healthcheck": "^0.1.0",
		"jsonwebtoken": "^8.5.1",
		"mysql2": "^2.3.2",
		"nodemon": "^2.0.14",
		"sequelize": "^6.7.0",
		"swagger-jsdoc": "^6.1.0",
		"swagger-ui-express": "^4.1.6",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"jest": "^27.3.1",
		"sequelize-cli": "^5.5.1",
		"supertest": "^6.1.6"
	}
